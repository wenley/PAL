{
    "name": "PAL",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Princeton Academic Linker",
    "background": {
        "page": "template.html"
    },
    "content_scripts": [
    {
        "matches": ["https://blackboard.princeton.edu/*"],
    	"exclude_matches": ["https://blackboard.princeton.edu/", "https://blackboard.princeton.edu/webapps/login/", 
"https://blackboard.princeton.edu/bbcswebdav/*", "https://blackboard.princeton.edu/@@/*"],
        "css": ["PAL.css"], 
    	"js": [
            "HTMLparser.js",
            "contentConnection.js",
            "pythonLib.js",
            "structures.js",
            "globalVariables.js",
            "prototypes.js",
            "mineGeneric.js",
            "mineAnnouncements.js",
            "mineContacts.js",
            "mineDescription.js",
            "mineDocuments.js",
            "mineSyllabus.js",
            "mineTools.js",
            "mineCourse.js",
            "miner.js",
            "toHTML.js",
            "expandSemesters.js",
            "populateTitle.js",
            "populateFromTab.js",
            "populateCourse.js",
            "populate.js",
            "render.js",
            "removeTab.js",
            "addTab.js"
        ]
      }
   ],
   "permissions": [
        "https://blackboard.princeton.edu/*",
        "background"
   ]
}